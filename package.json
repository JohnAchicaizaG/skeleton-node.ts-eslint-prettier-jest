    "test:coverage": "jest --coverage",
    "test:verbose": "jest --verbose",
    "format": "prettier --check 'src/**/*.{ts,tsx,js,jsx,json,md}'",
    "format:fix": "prettier --write 'src/**/*.{ts,tsx,js,jsx,json,md}'"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "dependencies": {
