    "globals": "^16.0.0",
    "jest": "^29.7.0",
    "pino-pretty": "^13.0.0",
    "prettier": "3.5.3",
    "rimraf": "^6.0.1",
    "ts-jest": "^29.2.6",
    "tsup": "^8.4.0",
    "tsx": "^4.19.3",
    "typescript-eslint": "^8.27.0"
